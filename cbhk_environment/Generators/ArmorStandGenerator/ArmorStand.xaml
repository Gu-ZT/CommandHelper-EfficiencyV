                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Content="生成覆盖"
                FontSize="15"
                Foreground="White"
                HeaderHeight="20"
                HeaderWidth="20"
                IsChecked="{Binding OverLying, Mode=OneWayToSource}"
                Style="{DynamicResource TextCheckBox}" />
            <CustomControls:IconTextButtons
                Width="18"
                Height="18"
                Margin="5,0"
                HorizontalAlignment="Left"
                Background="{StaticResource IssueIcon}"
                Style="{DynamicResource IconTextButton}"
                ToolTip="开启生成覆盖后生成结果将覆盖掉上一次的记录"
                ToolTipService.InitialShowDelay="0"
                ToolTipService.ShowDuration="3000" />
            <Menu Background="Transparent">
                <MenuItem
                    Width="20"
                    Height="18"
                    Margin="0,5,0,0"
                    HorizontalAlignment="Left"
                    BorderBrush="{x:Null}"
                    BorderThickness="0"
                    Cursor="Hand"
                    FlowDirection="LeftToRight"
                    Foreground="White"
                    ToolTip="切换版本"
                    ToolTipService.InitialShowDelay="0">
                    <MenuItem.Background>
                        <ImageBrush ImageSource="/cbhk_environment;component/resources/common/images/switch.png" />
                    </MenuItem.Background>
                    <CustomControls:TextToggleButtons
                        HorizontalAlignment="Stretch"
                        Background="{StaticResource DataButtonBackground}"
                        BorderThickness="0"
                        Content="1.8-"
                        Cursor="Hand"
                        FontSize="20"
                        Foreground="White"
                        IsChecked="{Binding Version1_8, Mode=TwoWay}"
                        SelectedBackground="{StaticResource DataButtonPressedBackground}"
                        Style="{StaticResource TextToggleButtonsStyle}" />
                    <CustomControls:TextToggleButtons
                        HorizontalAlignment="Stretch"
                        Background="{StaticResource DataButtonBackground}"
                        BorderThickness="0"
                        Content="1.9+"
                        Cursor="Hand"
                        FontSize="20"
                        Foreground="White"
                        IsChecked="{Binding Version1_9, Mode=TwoWay}"
                        SelectedBackground="{StaticResource DataButtonPressedBackground}"
                        Style="{StaticResource TextToggleButtonsStyle}" />
                </MenuItem>
            </Menu>

            <StackPanel
                x:Name="TitleStack"
                FlowDirection="RightToLeft"
                Orientation="Horizontal">
                <Button
                    Command="{Binding ReturnCommand}"
                    CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}"
                    Content="×"
                    Style="{StaticResource cbhk_form_btn}" />
                <!--<Button
                    Click="Window_StateChanged"
                    Content="□"
                    Style="{StaticResource cbhk_form_btn}" />-->
                <Button
                    Click="MinFormSize"
                    Content="-"
                    Style="{StaticResource cbhk_form_btn}" />
            </StackPanel>
        </DockPanel>
        <DockPanel
            MinHeight="570"
            HorizontalAlignment="Stretch"
            Background="Transparent"
            FlowDirection="LeftToRight"
            LastChildFill="True">
            <StackPanel Width="550" Background="Transparent">
