                        Width="90"
                        Height="30"
                        Margin="5,0,0,0"
                        HorizontalAlignment="Left"
                        Background="{StaticResource ButtonNormal}"
                        ClickMode="Press"
                        Command="{Binding ResetAllPoseCommand}"
                        Content="重置所有动作"
                        Foreground="{Binding Path=ResetAllPoseButtonForeground, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
                        PressedBackground="{StaticResource ButtonPressed}"
                        Style="{DynamicResource IconTextButton}" />
                    <CustomControls:IconTextButtons
                        Width="18"
                        Height="18"
                        Margin="5,0"
                        HorizontalAlignment="Left"
                        Background="{StaticResource IssueIcon}"
                        Style="{DynamicResource IconTextButton}"
