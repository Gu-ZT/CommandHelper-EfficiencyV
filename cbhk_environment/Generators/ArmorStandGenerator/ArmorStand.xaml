                        Padding="5,5,0,0"
                        HorizontalAlignment="Left"
                        Background="{StaticResource TextBoxBackground}"
                        BorderThickness="0"
                        CaretBrush="White"
                        FontSize="20"
                        Foreground="White"
                        Text="{Binding Path=CustomName, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}" />
