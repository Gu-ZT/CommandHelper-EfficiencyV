                    <TextBox
                        Width="100"
                        MinWidth="70"
                        MinHeight="35"
                        MaxWidth="130"
                        MaxHeight="35"
                        Margin="5,0"
                        Padding="5,5,0,0"
                        Background="{StaticResource TextBoxBackground}"
                        BorderThickness="0"
                        CaretBrush="White"
                        FontSize="20"
                        Foreground="White"
                        Text="{Binding Path=Tag, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}" />
