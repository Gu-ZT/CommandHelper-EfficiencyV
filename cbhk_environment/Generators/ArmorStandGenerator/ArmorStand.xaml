                        Width="35"
                        Margin="5,0"
                        Background="Black"
                        Cursor="Hand"
                        SelectColor="Black">
                        <ity:Interaction.Triggers>
                            <ity:EventTrigger EventName="PropertyChanged">
                                <itn:CallMethodAction MethodName="ColorPickersPropertyChanged" TargetObject="{Binding}" />
                            </ity:EventTrigger>
                        </ity:Interaction.Triggers>
                    </ColorPickers:ColorPickers>
                    <CustomControls:RadiusToggleButtons
                        Margin="5,0"
                        HorizontalAlignment="Left"
                        Cursor="Hand"
                        FontSize="12"
                        Foreground="White"
                        IsChecked="{Binding Path=CustomNameVisible, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged}"
                        SelectedToggleText="名称可见性"
                        Style="{StaticResource RadiusToggleButton}"
                        ToggleHeight="25"
                        ToggleWidth="45"
                        UnSelectedToggleText="名称可见性" />
                </DockPanel>
                <TextBlock
