                                        <DockPanel Grid.Row="0" Grid.Column="1">
                                            <TextBlock
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                FontSize="20"
                                                Text="Y" />
                                            <CustomControls:ColorNumbericUpDowns
                                                Height="35"
                                                Margin="0,0,5,0"
                                                HorizontalAlignment="Stretch"
                                                AllowDrop="False"
                                                ArrowBackground="Transparent"
                                                ArrowForeground="Black"
                                                ArrowHeight="25"
                                                ArrowWidth="25"
                                                Background="Transparent"
                                                BorderThickness="0"
                                                CaretBrush="White"
                                                FontSize="25"
                                                Foreground="Black"
                                                MaxValue="2147483647"
                                                MinValue="-2147483648"
                                                Template="{DynamicResource ColorNumbericUpDown}"
                                                Text="{Binding JobSiteY, Mode=TwoWay}" />
                                        </DockPanel>
                                        <DockPanel Grid.Row="0" Grid.Column="2">
                                            <TextBlock
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                FontSize="20"
                                                Text="Z" />
                                            <CustomControls:ColorNumbericUpDowns
                                                Height="35"
                                                HorizontalAlignment="Stretch"
                                                AllowDrop="False"
                                                ArrowBackground="Transparent"
                                                ArrowForeground="Black"
                                                ArrowHeight="25"
                                                ArrowWidth="25"
                                                Background="Transparent"
                                                BorderThickness="0"
                                                CaretBrush="White"
                                                FontSize="25"
                                                Foreground="Black"
                                                MaxValue="2147483647"
                                                MinValue="-2147483648"
                                                Template="{DynamicResource ColorNumbericUpDown}"
                                                Text="{Binding JobSiteZ, Mode=TwoWay}" />
                                        </DockPanel>
                                        <DockPanel
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="4">
                                            <TextBlock
                                                Margin="0,0,5,0"
                                                VerticalAlignment="Center"
                                                FontSize="20"
                                                Text="维度" />
                                            <CustomControls:TextComboBoxs
                                                Height="35"
                                                HorizontalAlignment="Stretch"
                                                AllowDrop="False"
                                                Background="Transparent"
                                                BorderThickness="0"
                                                FontSize="15"
                                                Foreground="White"
                                                SelectedItem="{Binding JobSiteDimension, Mode=TwoWay}"
                                                Style="{DynamicResource TextComboBox}">
                                                <ity:Interaction.Triggers>
                                                    <ity:EventTrigger EventName="Loaded">
                                                        <itn:CallMethodAction MethodName="DimensionTypeLoaded" TargetObject="{Binding}" />
                                                    </ity:EventTrigger>
                                                </ity:Interaction.Triggers>
                                            </CustomControls:TextComboBoxs>
                                        </DockPanel>
                                    </Grid>
                                </StackPanel>
                            </ScrollViewer>
                        </DockPanel>
                    </DockPanel>
                    <ScrollViewer
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        Background="#C6C6C6"
                        ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                        Style="{DynamicResource for_scrollviewer}">
                        <ItemsControl
                            HorizontalAlignment="Stretch"
                            ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                            ScrollViewer.VerticalScrollBarVisibility="Disabled">
                            <ity:Interaction.Triggers>
                                <ity:EventTrigger EventName="Loaded">
                                    <itn:CallMethodAction MethodName="ItemZoneLoaded" TargetObject="{Binding}" />
                                </ity:EventTrigger>
                            </ity:Interaction.Triggers>
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <VirtualizingStackPanel />
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </ItemsControl>
                    </ScrollViewer>
                </DockPanel>
                <DockPanel
                    Grid.Column="1"
                    Height="70"
                    Margin="55,0,25,100"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Bottom"
                    Panel.ZIndex="1"
                    Background="#C6C6C6"
                    LastChildFill="False">
                    <TextBlock
                        VerticalAlignment="Center"
                        FontSize="25"
                        Text="搜索言论类型" />
                    <CustomControls:TextComboBoxs
                        Width="150"
                        Height="35"
                        FontSize="25"
                        Style="{DynamicResource TextComboBox}">
                        <ity:Interaction.Triggers>
                            <ity:EventTrigger EventName="Loaded">
                                <itn:CallMethodAction MethodName="GossipSearchTypeLoaded" TargetObject="{Binding}" />
                            </ity:EventTrigger>
                        </ity:Interaction.Triggers>
                    </CustomControls:TextComboBoxs>
                    <TextBlock
                        VerticalAlignment="Center"
                        FontSize="25"
                        Text="搜索言论目标" />
                    <TextBox
                        Width="200"
                        Height="35"
                        Margin="5,0,0,0"
                        Background="Transparent"
                        BorderBrush="Black"
                        FontSize="25"
                        Foreground="Black">
                        <ity:Interaction.Triggers>
                            <ity:EventTrigger EventName="TextChanged">
                                <itn:CallMethodAction MethodName="SearchGossipsTextChanged" TargetObject="{Binding}" />
                            </ity:EventTrigger>
                            <ity:EventTrigger EventName="Loaded">
                                <itn:CallMethodAction MethodName="GossipSearchTargetLoaded" TargetObject="{Binding}" />
                            </ity:EventTrigger>
                        </ity:Interaction.Triggers>
                    </TextBox>
                    <TextBlock
                        VerticalAlignment="Center"
                        FontSize="25"
                        Text="搜索物品" />
                    <TextBox
                        Width="350"
                        Height="35"
                        Margin="5,0,0,0"
                        Background="Transparent"
                        BorderBrush="Black"
                        FontSize="25"
                        Foreground="Black">
                        <ity:Interaction.Triggers>
                            <ity:EventTrigger EventName="TextChanged">
                                <itn:CallMethodAction MethodName="SearchItemTextChanged" TargetObject="{Binding}" />
                            </ity:EventTrigger>
                            <ity:EventTrigger EventName="Loaded">
                                <itn:CallMethodAction MethodName="ItemSearcherLoaded" TargetObject="{Binding}" />
                            </ity:EventTrigger>
                        </ity:Interaction.Triggers>
                    </TextBox>
                </DockPanel>
                <DockPanel
                    Grid.Column="1"
                    Height="80"
                    Margin="55,0,25,25"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Bottom"
                    Panel.ZIndex="1"
                    AllowDrop="False"
                    Background="#C6C6C6">
                    <TextBlock
                        Margin="0,0,5,0"
                        VerticalAlignment="Center"
                        FontSize="25"
                        Foreground="Black"
                        Text="LastRestock"
                        ToolTip="此村民最后一次前往工作站点重新供应交易的刻。"
                        ToolTipService.InitialShowDelay="0"
                        ToolTipService.ShowDuration="1000" />
                    <CustomControls:ColorNumbericUpDowns
                        Width="450"
                        Height="50"
                        Margin="0,0,5,0"
                        HorizontalAlignment="Left"
                        AllowDrop="False"
                        ArrowBackground="Transparent"
                        ArrowForeground="Black"
                        ArrowHeight="25"
                        ArrowWidth="25"
                        Background="Transparent"
                        BorderThickness="0"
                        CaretBrush="White"
                        FontSize="25"
                        Foreground="Black"
                        MaxValue="2147483647"
                        MinValue="-2147483648"
                        Template="{DynamicResource ColorNumbericUpDown}"
                        Text="{Binding LastRestock, Mode=TwoWay}" />
                    <TextBlock
                        Margin="0,0,5,0"
                        VerticalAlignment="Center"
                        FontSize="25"
                        Foreground="Black"
                        Text="Xp"
                        ToolTip="此村民当前的经验值。"
                        ToolTipService.InitialShowDelay="0"
                        ToolTipService.ShowDuration="1000" />
                    <CustomControls:ColorNumbericUpDowns
                        Width="450"
                        Height="50"
                        HorizontalAlignment="Left"
                        AllowDrop="False"
                        ArrowBackground="Transparent"
                        ArrowForeground="Black"
                        ArrowHeight="25"
                        ArrowWidth="25"
                        Background="Transparent"
                        BorderThickness="0"
                        CaretBrush="White"
                        FontSize="25"
                        Foreground="Black"
                        MaxValue="2147483647"
                        MinValue="-2147483648"
                        Template="{DynamicResource ColorNumbericUpDown}"
                        Text="{Binding Xp, Mode=TwoWay}" />
                </DockPanel>
                <Grid
                    Grid.Row="0"
                    Grid.Column="1"
                    Width="1085"
                    Height="720"
                    Margin="40,0,0,0"
                    Panel.ZIndex="0"
                    Background="#C6C6C6" />
            </Grid>
        </Viewbox>
    </Grid>
</local_window:CommonWindow>
