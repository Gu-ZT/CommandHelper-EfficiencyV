                    x:Name="user_frame_border"
                    Width="30"
                    Height="30"
                    Margin="10,0,0,0"
                    BorderThickness="2"
                    Cursor="Hand"
                    FlowDirection="LeftToRight">
                    <Border.Background>
                        <ImageBrush x:Name="user_frame" ImageSource="resources/cbhk_form/images/cb.ico" />
                    </Border.Background>
                </Border>
                <Menu Padding="0,10,0,0" Background="Transparent">
                    <MenuItem>
                        <MenuItem.Header>
                            <TextBlock Foreground="White" Text="设置" />
                        </MenuItem.Header>
                        <MenuItem Click="StatupItemClick">
                            <MenuItem.Header>
                                <TextBlock Foreground="White" Text="启动项" />
                            </MenuItem.Header>
                        </MenuItem>
                        <MenuItem Click="IndividualizationForm">
                            <MenuItem.Header>
                                <TextBlock Foreground="White" Text="个性化" />
                            </MenuItem.Header>
                        </MenuItem>
                    </MenuItem>
                    <MenuItem>
                        <MenuItem.Header>
                            <TextBlock Foreground="White" Text="更多" />
                        </MenuItem.Header>
                        <MenuItem.DataContext>
                            <main_form_data_context:More />
                        </MenuItem.DataContext>
                        <MenuItem Command="{Binding ConversationGroupCommand}">
                            <MenuItem.Header>
                                <TextBlock Foreground="White" Text="进群交流" />
                            </MenuItem.Header>
                        </MenuItem>
                        <MenuItem Command="{Binding FeedBackBugsCommand}">
